<anf>
  <globals>
    <drawing mode="fill" shading="gouraud" background="0.2 0.2 0.2 0" />
    <culling face="back" order="ccw" />
    <lighting doublesided="false" local="true" enabled="true" ambient="0.6 0.6 0.6 1" />
  </globals>

  <cameras initial="perspective1" >
    <perspective id="perspective1" near="1" far="200" angle="30" pos="0 20 20" target="20 0 0" />
    <ortho id="ortho1" direction="x" near="20" far="-20" left="-10" right="5" top="20" bottom="0" />
  </cameras>

  <lights>
    <light id="omniGrey" type="omni" enabled="false" marker="false" pos="10 5 10" >
      <component type="ambient" value="0.2 0.2 0.2 0.1" />
      <component type="diffuse" value="0.2 0.2 0.2 0.1" />
      <component type="specular" value="0.2 0.2 0.2 0.1" />
    </light>
    <light id="spotWhite" type="spot" enabled="true" marker="false" pos="12 8 5" target="0 -1 0"
        angle="60" exponent="1" >
      <component type="ambient" value="0.8 0.8 0.8 1" />
      <component type="diffuse" value="0.8 0.8 0.8 1" />
      <component type="specular" value="0.8 0.8 0.8 1" />
    </light>
  </lights>

  <textures>

    <texture id="woodText" file="../data/woodText.jpg" texlength_s="1" texlength_t="1"/>
    <texture id="tab_lidText" file="../data/tzaar.jpg" texlength_s="1" texlength_t="1"/>

  </textures>
  <!-- bloco de materiais; -->
  <appearances>
    
    <appearance id="tabApp" shininess="0.2" textureref="woodText">
      <component type="ambient" value="0.65 0.5 0.39 1" />
      <component type="diffuse" value="0.65 0.5 0.39 1" />
      <component type="specular" value="0.65 0.5 0.39 1" />
    </appearance>
    
    <appearance id="tab_lidApp" shininess="0.2" textureref="tab_lidText">
      <component type="ambient" value="0.65 0.5 0.39 1" />
      <component type="diffuse" value="0.65 0.5 0.39 1" />
      <component type="specular" value="0.65 0.5 0.39 1" />
    </appearance>

  </appearances>


  <animations/>

  <graph rootid="tab">

    <node id="tab">
      <appearanceref id="tabApp"/>
      <transforms/>
      <descendants>
        
        <noderef id="tab_front"/>
        <noderef id="tab_back"/>
        <noderef id="tab_left"/>
        <noderef id="tab_right"/>
        <noderef id="tab_bottom"/>
        <noderef id="tab_lid"/>
      </descendants>
      <primitives/>
    </node>

    <node id="tab_lid">
      <appearanceref id="tab_lidApp"/>
      <!--set appearence-->
      <transforms>
        <transform type="translate" to="1 1 1"/>
        <transform type="scale" factor="2 1 2"/>
      </transforms>
      <descendants/>
      <primitives>
        <plane parts="30"/>
      </primitives>
    </node>

    <node id="tab_bottom">
      <transforms>
        <transform type="translate" to="1 0 1"/>
        <transform type="rotate" axis="x" angle="180"/>
        <transform type="scale" factor="2 1 2"/>
      </transforms>
      <descendants/>
      <primitives>
        <plane parts="30"/>
      </primitives>
    </node>

    <node id="tab_front">
      <transforms>
        <transform type="translate" to="1 0.5 2"/>
        <transform type="scale" factor="2 1 1"/>
        <transform type="rotate" axis="x" angle="90"/>
      </transforms>
      <descendants/>
      <primitives>
        <plane parts="30"/>
      </primitives>
    </node>

    <node id="tab_back">
      <appearanceref id="inherit"/>
      <transforms>
        <transform type="translate" to="1 0.5 0"/>
        <transform type="scale" factor="2 1 1"/>
        <transform type="rotate" axis="x" angle="-90"/>
      </transforms>
      <descendants/>
      <primitives>
        <plane parts="30"/>
      </primitives>
    </node>

    <node id="tab_left">
      <appearanceref id="inherit"/>
      <transforms>
        <transform type="translate" to="0 0.5 1"/>
        <transform type="scale" factor="1 1 2"/>
        <transform type="rotate" axis="z" angle="90"/>
      </transforms>
      <descendants/>
      <primitives>
        <plane parts="30"/>
      </primitives>
    </node>

    <node id="tab_right">
      <appearanceref id="inherit"/>
      <transforms>
        <transform type="translate" to="2 0.5 1"/>
        <transform type="scale" factor="1 1 2"/>
        <transform type="rotate" axis="z" angle="-90"/>
      </transforms>
      <descendants/>
      <primitives>
        <plane parts="30"/>
      </primitives>
    </node>


  </graph>




</anf>